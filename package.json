{
    "name": "jeckoxville",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-ga": "^2.6.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "0.9.x"
    },
    "devDependencies": {
        "node-sass": "^4.12.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "scripts": {
        "build-css": "node-sass src/ -o src/",
        "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
        "start": "react-scripts start npm run build-css && node-sass src/ -o src/ --watch --recursive",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    }
}
